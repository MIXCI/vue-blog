{
    "cSpell.words": [
      "Brotli",
      "caniuse",
      "commitlint",
      "CSSOM",
      "docsearch",
      "esbuild",
      "frontmatter",
      "iconify",
      "instanceof",
      "nightwatch",
      "Pinia",
      "proto",
      "shiki",
      "Tongji",
      "typeof"
    ],
    "path-intellisense.mappings": {
      "/": "${workspaceFolder}/public"
    },
    "search.exclude": {
      "**/node_modules": true,
      "**/bower_components": true,
      "**/*.code-search": true,
      ".vscode": true,
      "**/.temp": true,
      "docs/**": true,
      "yarn.lock": true,
    }
}
